<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.farlop.testfbconnect"
        versionCode="1" 
        version   = "0.0.1">

	<gap:config-file platform="ios" parent="CFBundleShortVersionString">
		<string>1</string>
	</gap:config-file>
	
    <name>Test FB Connect</name>

    <description>
        Test Crashing FB connect plugin
    </description>

    <author email="farlop@gmail.com">
        Farlop
    </author>

	<gap:plugin name="com.phonegap.plugins.facebookconnect">
		<param name="APP_ID" value="569665233063032" />
		<param name="APP_NAME" value="distrito" />
	</gap:plugin>
	<gap:plugin name="com.phonegap.plugin.statusbar" />	
	
	<preference name="phonegap-version" value="3.6.3" />
	<preference name="webviewbounce" value="false" />
	<preference name="ios-statusbarstyle" value="black-opaque" />
	<preference name="show-splash-screen-spinner" value="false" />
	<preference name="android-minSdkVersion" value="11" />	
	<preference name="permissions" value="none"/>
	<preference name="StatusBarOverlaysWebView" value="false" />
	
	<gap:platform name="ios" />
	<gap:platform name="android" />
</widget>